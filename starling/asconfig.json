{
	"type": "lib",
	"config": "royale",
	"compilerOptions": {
		"targets": [
			"SWF",
			"JS"
		],
		"output": "bin/starling-royale.swc",
		"source-path": [
			"src"
		],
		"js-external-library-path": [
			"libs/openfl.swc"
		],
		"swf-external-library-path": [
			"${royalelib}/../js/libs/js.swc",
			"${royalelib}/../js/libs/GCL.swc",
			"${royalelib}/js/libs/LanguageJS.swc",
			"${royalelib}/js/libs/CoreJS.swc",
			"${royalelib}/js/libs/XMLJS.swc",
			"${royalelib}/js/libs/ReflectionJS.swc",
			"libs/openfl.swc"
		],
		"define": [
			{"name": "COMPILE::SWF", "value": false},
			{"name": "COMPILE::JS",  "value": true }
		],
		"include-sources": [
			"src"
		],
		"js-vector-emulation-class": "openfl.Vector"
	},
	"additionalOptions": [
		"-js-getter-prefix=get_",
		"-js-setter-prefix=set_"
	]
}